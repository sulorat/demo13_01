<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
		x:CompileBindings="False"
        x:Class="demo1301.MainWindow"
        Title="demo1301">
	<Border Background="Aqua">
		<StackPanel HorizontalAlignment="Center">
			
			<TextBlock Text="Регистрация" Margin="30,50"/>
			<TextBlock Text="Имя" Margin="10"/>
			<TextBox Width="150"/>
			<TextBlock Text="Пароль" Margin="10" />
			<TextBox Width="150" Name="PasswordTextBox"/>

			<StackPanel>
				<Button Name="ToMenuWindowButton" Click="ToMenuClickButton" Content="Войти" Margin="40"/>
			</StackPanel>
		</StackPanel>
	</Border>
</Window>
